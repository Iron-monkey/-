.search {
  height: 60rpx;
  background-color: #e4e4e4;
  padding: 20rpx;
  position: relative;
}
.search_icon {
  position: absolute;
  left: 25rpx;
  top: 25rpx;
  width: 60rpx;
  height: 60rpx;
}
.search_icon image {
  width: 90%;
  height: 90%;
}
.search input {
  width: 90%;
  height: 60rpx;
  border-radius: 10rpx;
  background-color: #fff;
  padding-left: 60rpx;
  line-height: 60rpx;
}
/* 组织列表页面 */
.groupImg {
  margin-top: 20px;
  width: 100%;
  height: 60px;
  display:flex;
  padding: 0 30rpx;
  align-items: center;
}
.groupImg image {
  flex: 1;
  width: 50px;
  margin-right: 20rpx;
}
.group-name{
  flex: 4;
  float: left;
  margin-left: 5px;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.action {
  font-size: 12px;
}
.huoyuedu {
  margin-left: 20px;
  font-size: 12px;
}
